{
    "name": "brightlabs/craft-securepay",
    "description": "SecurePay payment gateway for Craft Commerce",
    "type": "craft-plugin",
    "version": "1.3.2",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft-commerce",
        "payment",
        "gateway",
        "securepay"
    ],
    "support": {
        "docs": "https://bitbucket.org/bright-labs/craftcommerce-securepay/src/master/README.md",
        "issues": "https://bitbucket.org/bright-labs/craftcommerce-securepay/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Brightlabs",
            "homepage": "https://brightlabs.com.au/"
        }
    ],
    "require": {
        "php": "^8.2",
        "craftcms/cms": "~5.7",
        "craftcms/commerce": "~5.3",
        "guzzlehttp/guzzle": "^7.0",
        "fgct/securepay-api": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "brightlabs\\securepay\\": "src/"
        }
    },
    "extra": {
        "name": "SecurePay for Craft Commerce",
        "handle": "securepay",
        "developer": "Brightlabs",
        "developerUrl": "https://brightlabs.com.au/",
        "documentationUrl": "https://bitbucket.org/bright-labs/craftcommerce-securepay/src/master/README.md",
        "changelogUrl": "https://bitbucket.org/bright-labs/craftcommerce-securepay/src/master/CHANGELOG.md",
        "class": "brightlabs\\securepay\\Plugin"
    }
} 